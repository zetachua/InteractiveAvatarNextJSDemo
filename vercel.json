{
    "version": 2,
    "builds": [
      {
        "src": "api/transcribe.py",
        "use": "@vercel/python"
      },
      {
        "src": "api/audio_analysis.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/transcribe",
        "dest": "api/transcribe.py"
      },
      {
        "src": "/audio_analysis",
        "dest": "api/audio_analysis.py"
      }
    ]
  }
  